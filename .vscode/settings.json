{
  "editor.tabSize": 2,
  "cSpell.words": [
    "Haco",
    "Lazp",
    "Linkify",
    "MDSIG",
    "Meru",
    "Mingdao",
    "Swal",
    "sweetalert",
    "webp"
  ],
  "commit-ai.prompt.openAiTemperature": 0.5,
  "commit-ai.prompt.openAiMaxToken": 100,
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "deepscan.enable": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  }
}