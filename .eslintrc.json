{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@next/next/no-img-element": "off",
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "brace-style": [
      "error",
      "1tbs"
    ],
    "no-unused-vars": "off"
  },
  "extends": [
    "eslint:recommended",
    "next"
  ]
}